   ],
   "source": [
    "from datetime import datetime as dt\n",
    "to_year = lambda x: dt.strptime(x, \"%Y%m%d\").year\n",
    "\n",
    "years = np.loadtxt('./data/KNMI.txt', delimiter=',', usecols=(1), unpack=True,\n",
    "                  converters={1: to_year}, encoding='latin1', skiprows=1)\n",
    "\n",
    "print ('first year: ', years.min(), ' and last year: ', years.max())"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "and we will draw a graph of mean tempreture by year"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
